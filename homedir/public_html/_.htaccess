RewriteEngine on
RewriteCond $1 !^(index\.php|resources|robots\.txt)
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php/$1 [L,QSA] 
RewriteCond %{HTTP_HOST} ^uniprev\.com\.br$ [OR]
RewriteCond %{HTTP_HOST} ^www\.uniprev\.com\.br$
RewriteRule ^/?$ "http\:\/\/uniprev\.com\.br\/home\/inicio" [R=302,L]

